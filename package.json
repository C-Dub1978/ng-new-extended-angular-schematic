{
  "name": "sdi-angular-schematic",
  "version": "1.0.0",
  "description": "A schematic to scaffold GCI Angular project directory structure",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch"
