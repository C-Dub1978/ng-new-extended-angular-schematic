  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.2.4",
    "@angular-devkit/schematics": "^12.2.4",
    "@schematics/angular": "^12.2.7",
    "typescript": "~4.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0",
    "prettier": "^2.4.1"
  }
}
